<?php

use Drupal\Core\Routing\RouteMatchInterface;

function tpedunews_help($route_name, RouteMatchInterface $route_match)
{
    switch ($route_name) {
        case 'help.page.tpedunews':
            $output = '<p>本模組將為您新增教育局最新消息新聞聯播，並在主導覽選單為您新增「教育局最新消息」項目。</p>';

            return $output;
    }
}
